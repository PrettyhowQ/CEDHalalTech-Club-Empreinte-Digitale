# Club Empreinte Digitale - Variables d'environnement
# Copiez ce fichier vers .env et remplissez les valeurs

# Base de données PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/club_empreinte_digitale

# OpenAI API pour Super IARP Pro
OPENAI_API_KEY=votre_cle_openai_ici

# Configuration Replit (optionnel pour développement local)
REPL_ID=club-empreinte-digitale
REPLIT_DOMAINS=club-empreinte-digitale.replit.app

# Configuration de session
SESSION_SECRET=votre_secret_session_ultra_securise_ici

# Configuration Google Calendar (optionnel)
GOOGLE_CLIENT_ID=votre_client_id_google
GOOGLE_CLIENT_SECRET=votre_secret_google

# Environnement de développement
NODE_ENV=development
PORT=5000